<launch>
    <!-- 使用仿真时间 -->
    <param name="use_sim_time" value="true" />
        <!-- 启动 Gazebo 的仿真环境，当前环境为空环境 -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(find robot_description)/worlds/world.world" />
    </include>
    
</launch>
